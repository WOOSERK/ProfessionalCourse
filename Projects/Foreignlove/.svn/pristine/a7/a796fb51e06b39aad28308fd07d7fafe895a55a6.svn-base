package kr.co.foreignlove.vo;

public class BoardPromotionVO
{
	private int p_id;
	private String p_title;
	private String p_content;
	private String p_attached; // 첨부파일
	private String p_post; // 작성일자
	private String p_delete; // 삭제일자
	private int p_count; // 조회수
	private String p_startDate; // 행사 시작일자
	private String p_endDate; // 행사 종료일자
	private String bt_type; // 게시판 종류
	private String p_type; // 행사 종류

	public int getP_id()
	{
		return p_id;
	}

	public void setP_id(int p_id)
	{
		this.p_id = p_id;
	}

	public String getP_title()
	{
		return p_title;
	}

	public void setP_title(String p_title)
	{
		this.p_title = p_title;
	}

	public String getP_content()
	{
		return p_content;
	}

	public void setP_content(String p_content)
	{
		this.p_content = p_content;
	}

	public String getP_attached()
	{
		return p_attached;
	}

	public void setP_attached(String p_attached)
	{
		this.p_attached = p_attached;
	}

	public String getP_post()
	{
		return p_post;
	}

	public void setP_post(String p_post)
	{
		this.p_post = p_post;
	}

	public String getP_delete()
	{
		return p_delete;
	}

	public void setP_delete(String p_delete)
	{
		this.p_delete = p_delete;
	}

	public int getP_count()
	{
		return p_count;
	}

	public void setP_count(int p_count)
	{
		this.p_count = p_count;
	}

	public String getP_startDate()
	{
		return p_startDate;
	}

	public void setP_startDate(String p_startDate)
	{
		this.p_startDate = p_startDate;
	}

	public String getP_endDate()
	{
		return p_endDate;
	}

	public void setP_endDate(String p_endDate)
	{
		this.p_endDate = p_endDate;
	}

	public String getBt_type()
	{
		return bt_type;
	}

	public void setBt_type(String bt_type)
	{
		this.bt_type = bt_type;
	}

	public String getP_type()
	{
		return p_type;
	}

	public void setP_type(String p_type)
	{
		this.p_type = p_type;
	}

	@Override
	public int hashCode()
	{
		final int prime = 31;
		int result = 1;
		result = prime * result + ((p_attached == null) ? 0 : p_attached.hashCode());
		result = prime * result + ((bt_type == null) ? 0 : bt_type.hashCode());
		result = prime * result + ((p_content == null) ? 0 : p_content.hashCode());
		result = prime * result + p_count;
		result = prime * result + ((p_delete == null) ? 0 : p_delete.hashCode());
		result = prime * result + ((p_endDate == null) ? 0 : p_endDate.hashCode());
		result = prime * result + p_id;
		result = prime * result + ((p_post == null) ? 0 : p_post.hashCode());
		result = prime * result + ((p_startDate == null) ? 0 : p_startDate.hashCode());
		result = prime * result + ((p_title == null) ? 0 : p_title.hashCode());
		result = prime * result + ((p_type == null) ? 0 : p_type.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj)
	{
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		BoardPromotionVO other = (BoardPromotionVO) obj;
		if (p_attached == null)
		{
			if (other.p_attached != null)
				return false;
		} else if (!p_attached.equals(other.p_attached))
			return false;
		if (bt_type == null)
		{
			if (other.bt_type != null)
				return false;
		} else if (!bt_type.equals(other.bt_type))
			return false;
		if (p_content == null)
		{
			if (other.p_content != null)
				return false;
		} else if (!p_content.equals(other.p_content))
			return false;
		if (p_count != other.p_count)
			return false;
		if (p_delete == null)
		{
			if (other.p_delete != null)
				return false;
		} else if (!p_delete.equals(other.p_delete))
			return false;
		if (p_endDate == null)
		{
			if (other.p_endDate != null)
				return false;
		} else if (!p_endDate.equals(other.p_endDate))
			return false;
		if (p_id != other.p_id)
			return false;
		if (p_post == null)
		{
			if (other.p_post != null)
				return false;
		} else if (!p_post.equals(other.p_post))
			return false;
		if (p_startDate == null)
		{
			if (other.p_startDate != null)
				return false;
		} else if (!p_startDate.equals(other.p_startDate))
			return false;
		if (p_title == null)
		{
			if (other.p_title != null)
				return false;
		} else if (!p_title.equals(other.p_title))
			return false;
		if (p_type == null)
		{
			if (other.p_type != null)
				return false;
		} else if (!p_type.equals(other.p_type))
			return false;
		return true;
	}

	@Override
	public String toString()
	{
		return "BoardPromotionVO [p_id=" + p_id + ", p_title=" + p_title + ", p_content=" + p_content + ", p_attached="
				+ p_attached + ", p_post=" + p_post + ", p_delete=" + p_delete + ", p_count=" + p_count + ", p_startDate="
				+ p_startDate + ", p_endDate=" + p_endDate + ", bt_type=" + bt_type + ", p_type=" + p_type + "]";
	}
}
